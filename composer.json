{
    "name": "reacted/reacted",
    "description": "A Micro PHP Framework with ReactPHP",
    "version": "0.0.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Naman Kumar",
            "email": "namankumar80510@gmail.com"
        }
    ],
    "require": {
        "dikki/dotenv": "^2.0",
        "laminas/laminas-diactoros": "^3.5",
        "laminas/laminas-httphandlerrunner": "^2.11",
        "league/plates": "^3.6",
        "league/route": "^6.2",
        "react/async": "^1.0",
        "react/event-loop": "^0.4.3",
        "react/http": "^0.4.0",
        "react/promise": "^3.2",
        "symfony/dependency-injection": "^7.1",
        "tracy/tracy": "^2.10"
    },
    "require-dev": {
        "nette/tester": "^2.5",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Reacted\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "scripts": {
        "test": "php vendor/bin/tester",
        "check": "php vendor/bin/phpstan analyse",
        "check-cs": "phpcs --standard=phpcs.xml.dist",
        "fix-cs": "phpcbf --standard=phpcs.xml.dist"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
